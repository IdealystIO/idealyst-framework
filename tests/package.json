{
  "name": "idealyst-cli-tests",
  "version": "1.0.0",
  "description": "Comprehensive test suite for Idealyst CLI",
  "private": true,
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:manual": "node manual-cli-test.js",
    "test:demo": "jest demo.test.ts",
    "test:all": "yarn test:demo && yarn test:manual",
    "build-cli": "cd ../packages/cli && yarn build",
    "prep": "yarn build-cli && echo 'CLI built successfully. Ready for testing.'"
  },
  "dependencies": {
    "fs-extra": "^11.0.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "testing",
    "cli",
    "idealyst",
    "react-native",
    "react",
    "typescript"
  ]
} 