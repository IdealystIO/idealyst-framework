{
  "name": "idealyst-monorepo",
  "version": "1.0.61",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/IdealystIO/idealyst-framework.git"
  },
  "workspaces": [
    "packages/*"
  ],
  "packageManager": "yarn@4.9.4",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "publish:theme": "cd packages/theme && npm publish",
    "publish:components": "cd packages/components && npm publish",
    "publish:navigation": "cd packages/navigation && npm publish",
    "publish:cli": "cd packages/cli && npm publish",
    "publish:datagrid": "cd packages/datagrid && npm publish",
    "publish:storage": "cd packages/storage && npm publish",
    "publish:all": "yarn workspaces foreach -A --no-private -i -j 1 npm publish --tolerate-republish --otp",
    "version:patch": "yarn workspaces foreach -A --no-private version patch && yarn version:sync",
    "version:minor": "yarn workspaces foreach -A --no-private version minor && yarn version:sync",
    "version:major": "yarn workspaces foreach -A --no-private version major && yarn version:sync",
    "version:sync": "node scripts/sync-versions.js",
    "version:set": "node scripts/set-version.js",
    "version:status": "node scripts/version-status.js",
    "version:alpha": "node scripts/set-version.js",
    "version:beta": "node scripts/set-version.js",
    "version:rc": "node scripts/set-version.js"
  },
  "resolutions": {
    "@react-navigation/native": "7.1.14",
    "@react-navigation/bottom-tabs": "7.4.2",
    "@react-navigation/drawer": "7.5.3",
    "@react-navigation/native-stack": "7.3.21",
    "react": "19.1.0",
    "react-native": "0.80.1",
    "react-native-unistyles": "^3.0.10",
    "react-native-safe-area-context": "^5.5.1",
    "react-native-screens": "^4.11.1",
    "react-native-gesture-handler": "^2.27.1",
    "react-native-reanimated": "^3.18.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.27.6"
  },
  "devDependencies": {
    "@babel/compat-data": "^7.28.0",
    "@babel/core": "^7.28.0",
    "@babel/helper-compilation-targets": "^7.27.2",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@types/fs-extra": "^11.0.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.0.0",
    "fs-extra": "^11.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.0.0"
  }
}
