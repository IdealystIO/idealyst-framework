{
  "name": "@idealyst/mcp-server",
  "version": "1.1.4",
  "description": "MCP server providing documentation and examples for the Idealyst framework",
  "main": "dist/index.js",
  "bin": {
    "idealyst-mcp": "./dist/index.js"
  },
  "type": "module",
  "files": [
    "dist/**/*",
    "examples/**/*",
    "README.md"
  ],
  "scripts": {
    "extract-types": "tsx scripts/extract-types.ts",
    "validate-examples": "tsx scripts/validate-examples.ts",
    "test": "tsx scripts/test-examples.ts",
    "prebuild": "yarn extract-types && yarn validate-examples",
    "build": "tsc && chmod +x dist/index.js && mkdir -p dist/generated && cp -r src/generated/* dist/generated/",
    "dev": "tsc --watch",
    "start": "node dist/index.js"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "keywords": [
    "idealyst",
    "mcp",
    "model-context-protocol",
    "documentation",
    "framework"
  ],
  "author": "Idealyst",
  "license": "MIT",
  "dependencies": {
    "@idealyst/components": "^1.1.4",
    "@idealyst/navigation": "^1.1.4",
    "@idealyst/theme": "^1.1.4",
    "@modelcontextprotocol/sdk": "^1.0.4"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^19.1.0",
    "chalk": "^5.3.0",
    "react": "^19.1.0",
    "react-native": "^0.80.1",
    "ts-morph": "^21.0.1",
    "tsx": "^4.7.0",
    "typescript": "^5.0.0"
  }
}
