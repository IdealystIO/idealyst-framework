{
  "name": "@idealyst/oauth-client",
  "version": "1.2.45",
  "description": "Universal OAuth2 client for web and React Native",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "types": "src/index.ts",
  "react-native": "src/index.native.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "react-native": "./src/index.native.ts",
      "browser": "./src/index.web.ts",
      "import": "./src/index.ts",
      "require": "./src/index.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "yarn build"
  },
  "keywords": [
    "oauth",
    "oauth2",
    "authentication",
    "react",
    "react-native",
    "cross-platform"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@idealyst/storage": "^1.2.30",
    "@types/react": "^18.3.18",
    "@types/react-native": "^0.73.0",
    "react": "^19.1.0",
    "react-native": "^0.80.1",
    "tsup": "^8.3.5",
    "typescript": "^5.7.3"
  },
  "peerDependencies": {
    "@idealyst/storage": "^1.2.30",
    "react-native": ">=0.60.0"
  },
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  }
}
