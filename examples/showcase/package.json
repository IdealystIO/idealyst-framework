{
  "name": "@idealyst-showcase/showcase",
  "version": "1.0.0",
  "description": "Multi-screen Idealyst Framework showcase app",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "packageManager": "yarn@4.1.0",
  "scripts": {
    "dev": "yarn workspace @idealyst-showcase/web dev",
    "build": "yarn workspace @idealyst-showcase/web build",
    "preview": "yarn workspace @idealyst-showcase/web preview",
    "start:mobile": "yarn workspace @idealyst-showcase/mobile start",
    "android": "yarn workspace @idealyst-showcase/mobile android",
    "ios": "yarn workspace @idealyst-showcase/mobile ios",
    "api": "yarn workspace @idealyst-showcase/api dev",
    "api:build": "yarn workspace @idealyst-showcase/api build",
    "db:generate": "yarn workspace @idealyst-showcase/database db:generate",
    "db:push": "yarn workspace @idealyst-showcase/database db:push",
    "db:migrate": "yarn workspace @idealyst-showcase/database db:migrate",
    "db:studio": "yarn workspace @idealyst-showcase/database db:studio",
    "db:seed": "yarn workspace @idealyst-showcase/database db:seed"
  }
}
